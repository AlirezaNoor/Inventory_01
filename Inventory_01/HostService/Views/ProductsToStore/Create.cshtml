@model InventoryApplicationContract.Addedproductstostore.CreateAddPtoS

<div class="page-wrapper">
    <div class="content">
        <div class="page-header">
            <div class="page-title">
                <h4>افزودن به انبار</h4>
            </div>
        </div>
        <form method="post">
            <div class="card">
                <div class="card-body">
                    <div class="row">
                        <div class="col-lg-3 col-sm-6 col-12">
                            <div class="form-group">
                                <label>کالا</label>
                                <select class="select" asp-for="ProductRef" asp-items="@Model.Product">
                                    <option>Choose Country</option>
                                </select>
                            </div>
                        </div>
                        <div class="col-lg-3 col-sm-6 col-12">
                            <div class="form-group">
                                <label>انبار</label>
                                <select class="select" asp-for="StoreRef" asp-items="@Model.Store">
                                    <option>Choose Country</option>
                                </select>
                            </div>
                        </div>

                        <div class="col-lg-12">
                            <input type="submit" placeholder="submit" class="btn btn-submit me-2">
                            <a href="storelist.html" class="btn btn-cancel">Cancel</a>
                        </div>
                    </div>
                </div>
            </div>


        </form>


    </div>
</div>